{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "libs"
    },
    {
      "dir": "features/steps",
      "type": "dev"
    },
    {
      "dir": "tests",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "commonjs"
  },
  "suffix": ".bs.js",
  "bs-dependencies": ["@ryyppy/rescript-promise"],
  "bs-dev-dependencies": ["@dusty-phillips/rescript-zora"],
  "gentypeconfig": {
    "language": "typescript",
    "module": "commonjs",
    "shims": {},
    "exportInterfaces": false,
    "generatedFileExtension": ".gen.ts"
  },
  "warnings": {
    "error": "+101"
  }
}
