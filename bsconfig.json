{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "features/steps"
    },
    {
      "dir": "libs"
    },
    {
      "dir": "tests",
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs"
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@ryyppy/rescript-promise"
  ],
  "bs-dev-dependencies": [
    "rescript-test"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "module": "commonjs",
    "shims": {},
    "exportInterfaces": false,
    "generatedFileExtension": ".gen.ts"
  },
  "warnings": {
    "error": "+101"
  }
}